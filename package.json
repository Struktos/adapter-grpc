{
  "name": "@struktos/adapter-grpc",
  "version": "0.1.2",
  "description": "gRPC adapter for Struktos.js - Enterprise-grade gRPC integration with context propagation and middleware support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "dev": "tsx watch src/index.ts",
    "example": "tsx examples/basic-server.ts",
    "example:client": "tsx examples/basic-client.ts",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "release": "./scripts/release.sh",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major"
  },
  "keywords": [
    "struktos",
    "grpc",
    "adapter",
    "microservices",
    "msa",
    "rpc",
    "protobuf",
    "context-propagation",
    "interceptor",
    "middleware",
    "enterprise",
    "typescript"
  ],
  "author": "Struktos.js Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/struktos/adapter-grpc"
  },
  "bugs": {
    "url": "https://github.com/struktos/adapter-grpc/issues"
  },
  "homepage": "https://github.com/struktos/adapter-grpc#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@struktos/core": "^1.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.5",
    "@grpc/proto-loader": "^0.7.13"
  },
  "devDependencies": {
    "@struktos/core": "^1.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  }
}
